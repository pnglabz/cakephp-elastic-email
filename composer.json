{
    "name": "pnglabz/cakephp-elastic-email",
    "description": "ElasticEmail plugin for CakePHP 3 - Send transactional emails using Elastic Email API",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "CakePHP",
        "Email",
        "ElasticEmail"
    ],
    "authors": [
        {
            "name": "PNG Labz",
            "homepage": "https://www.pnglabz.com",
            "role": "Author"
        },
        {
            "name": "Narendra Vaghela",
            "homepage": "http://www.narendravaghela.com",
            "role": "Author"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/pnglabz/cakephp-rest/graphs/contributors"
        }
    ],
    "support": {
        "source": "https://github.com/pnglabz/cakephp-elastic-email",
        "issues": "https://github.com/pnglabz/cakephp-elastic-email/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ElasticEmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElasticEmail\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
